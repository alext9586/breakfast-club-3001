{
  "name": "breakfast-club-3001",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "cd client && npm install",
    "client": "cd client && npm run start",
    "server": "node ./bin/www",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "npm run dev",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "http-errors": "~1.6.2",
    "morgan": "^1.9.1",
    "pg-promise": "^8.4.0",
    "pug": "^2.0.3"
  },
  "devDependencies": {
    "@types/node": "^9.6.7",
    "concurrently": "^3.5.0",
    "deep-extend": "^0.6.0",
    "nodemon": "^1.18.10"
  },
  "engines": {
    "node": "11.x"
  }
}
